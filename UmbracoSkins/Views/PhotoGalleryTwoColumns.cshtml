@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "TwoColumn.cshtml";
}

@section script {
    <script src="/js/libs/bjqs-1.3.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function (e) {
            $('#photoSlider').bjqs({
                height: 320,
                width: 620,
                responsive: true
            });
        });
    </script>
}

@section css{
    <link href="/css/bjqs.css" rel="stylesheet" type="text/css" />
}

<div id="photoSlider">
@Umbraco.RenderMacro("PhotoGalleryImplementation")
</div>