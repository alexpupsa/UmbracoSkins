@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
}

<!doctype html>
<html>
<head>
    <title>Two column</title>
    <script src="/js/libs/jquery-1.8.3.min.js" type="text/javascript"></script>
    <script src="/js/libs/jquery.easing.1.3.js" type="text/javascript"></script>
    <script src="/js/libs/jquery.mobile.customized.min.js" type="text/javascript"></script>

    @RenderSection("css", false)

    <link href="/css/two-columns.css" rel="stylesheet" type="text/css" />

    @RenderSection("script", false)
</head>
<body>
    <div id="divContent">
        <div id="divHeader">
           <p class="site-title"><a href="http://example.com">Site Title</a></p>
           <p class="site-description">Site desciption...</p>
        </div>
        <div id="divMenu">
            <div id="divInsideMenu">
                @Umbraco.RenderMacro("MenuItems")
            </div>
        </div>
        <div id="divContentRight">
            @RenderBody()
        </div>
    </div>
    <div id="divFooter">
        <p class="site-credits">&copy; Copyright owner</p>
    </div>
</body>
</html>